<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="418.0" prefWidth="753.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <center>
      <HBox prefHeight="418.0" prefWidth="604.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btn" mnemonicParsing="false" onAction="#printUsers" text="Afficher tous les utilisateurs" />
            <TableView fx:id="table" prefHeight="418.0" prefWidth="451.0">
              <columns>
                <TableColumn fx:id="idUtilisateur" prefWidth="75.0" text="idUtilisateur" />
                <TableColumn fx:id="nom" prefWidth="75.0" text="nom" />
                  <TableColumn fx:id="prenom" prefWidth="75.0" text="prenom" />
                  <TableColumn fx:id="email" prefWidth="75.0" text="email" />
                  <TableColumn fx:id="telephone" prefWidth="75.0" text="telephone" />
                  <TableColumn fx:id="idAdresse" prefWidth="75.0" text="idAdresse" />
              </columns>
            </TableView>
         </children>
      </HBox>
   </center>
</BorderPane>
